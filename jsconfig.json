{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@Components/*": ["src/components/*"],
      "@Assets/*": ["src/assets/*"],
      "@Utils/*": ["src/utils/*"],
      "@Layouts/*": ["src/layouts/*"],
      "@Pages/*": ["src/pages/*"],
      "@Store/*": ["src/store/*"],
      "@Api/*": ["src/api/*"],
      "@UIkit/*": ["src/UIkit/*"],
      "@Routes/*": ["src/routes/*"],
      "@Helpers/*": ["src/helpers/*"]
    },
    "target": "ES6",
    "jsx": "preserve",
    "allowSyntheticDefaultImports": true
  }
}
